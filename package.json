{
  "name": "in-search-of-stuff",
  "version": "0.0.1",
  "description": "",
  "author": "Jacob Barnett",
  "license": "UNLICENSED",
  "homepage": "https://github.com/jacoblb64/in-search-of-stuff#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacoblb64/in-search-of-stuff.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm-run-all build:content build:styles",
    "build:prod": "npm-run-all build:content build:styles:prod",
    "build:content": "eleventy",
    "build:styles": "postcss src/styles/tailwind.pcss -o dist/styles/tailwind.css",
    "build:styles:prod": "npm run build:styles -- --env=production",
    "watch:content": "eleventy --serve",
    "watch:styles": "npm run build:styles -- --watch",
    "start": "npm-run-all --parallel watch:content watch:styles"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "autoprefixer": "^9.7.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.4.6"
  }
}
